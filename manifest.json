{
	"manifest_version": 2,
	"name": "BBOalert-criptik",
	"description": "Auto alert on BBO",
	"version": "5.0.902",
	"permissions": [
		"clipboardRead",
		"clipboardWrite"
	],
	"icons": {
		"16": "BBOalert16.png",
		"48": "BBOalert48.png",
		"128": "BBOalert128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://www.bridgebase.com/v3/*",
				"*://webutil.bridgebase.com/v2/*"
			],
			"css": ["BBOalert.css"],
			"js": [ 
				"jquery-3.5.1.min.js",
				"functions.js",
				"BBOalertData.js",
				"BBOobserver.js",
				"BBOalert.js"
			],
			"all_frames":true
		}
	]
}
